{
  "name": "<%=modulePackageName%>",
  "version": "1.0.0",
  "description": "",
  "contributors": [],
  "scripts": {
    "prebuild": "npm run clean",
    "build": "bundle-module",
    "watch:build": "npm run build -- --watch",
    "clean": "rimraf build",
    "prepare": "npm run build",
    "test:lint": "eslint --ignore-path .gitignore --ext js,jsx,snap .",
    "test:unit": "jest",
    "test:git-history": "commitlint --from origin/master --to HEAD",
    "test": "npm run test:lint && npm run test:unit && npm run test:git-history"
  },
  "dependencies": {
    "@americanexpress/one-app-bundler": "^6.0.0",
    "@americanexpress/one-app-router": "^1.0.0",
    "content-security-policy-builder": "^2.1.0",
    "holocron": "^1.0.0",
    "ip": "^1.1.5",
    "react": "^16.12.0"
  },
  "devDependencies": {
    "@commitlint/config-conventional": "^8.3.4",
    "amex-jest-preset-react": "^5.0.2",
    "babel-eslint": "^8.2.6",
    "babel-preset-amex": "^3.2.0",
    "commitlint": "^8.3.5",
    "enzyme": "^3.11.0",
    "enzyme-to-json": "^3.4.4",
    "eslint": "^6.8.0",
    "eslint-config-amex": "^11.1.0",
    "husky": "^4.2.1",
    "jest": "^25.1.0",
    "rimraf": "^3.0.0"
  },
  "jest": {
    "preset": "amex-jest-preset-react"
  },
  "husky": {
    "hooks": {
      "pre-push": "npm test",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  }
}
